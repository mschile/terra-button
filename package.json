{
  "name": "terra-button",
  "main": "lib/Button.js",
  "version": "0.2.0",
  "description": "The terra-button component provides users a way to trigger actions in the UI.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-button.git"
  },
  "keywords": [
    "Button",
    "Cerner",
    "Terra",
    "terra-button",
    "UI"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-button/issues"
  },
  "homepage": "https://github.com/cerner/terra-button#readme",
  "jest": {
    "setupFiles": [
      "./tests/jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "gh-pages": "^0.12.0",
    "github-markdown-css": "^2.4.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "markdown-it": "^8.2.2",
    "marked": "^0.3.6",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react-docgen": "^2.13.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-config-sass-guidelines": "^1.1.1",
    "terra-application": "^0.2.0",
    "terra-legacy-theme": "^0.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-component-props-table": "0.0.1",
    "react-docgen": "^2.13.0",
    "react-dom": "^15.4.2",
    "terra-mixins": "^1.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "deploy": "build-storybook && gh-pages -d storybook-static",
    "lint": "yarn lint-js && yarn lint-scss",
    "lint-js": "eslint --ext .js,.jsx src",
    "lint-scss": "stylelint src/**/*.scss",
    "pretest": "yarn lint",
    "release:major": "yarn test && yarn build && npm version major -m \"Released version %s\" && npm publish && git push --follow-tags",
    "release:minor": "yarn test && yarn build && npm version minor -m \"Released version %s\" && npm publish && git push --follow-tags",
    "release:patch": "yarn test && yarn build && npm version patch -m \"Released version %s\" && npm publish && git push --follow-tags",
    "start": "webpack-dev-server --hot --inline --display-error-details --progress --colors --host 0.0.0.0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  }
}
